---
import IdGroupMembers from '../../components/IdGroupMembers.astro';
import { loadStaticPaths } from '../../components/load-static-paths';

export async function getStaticPaths() { 
	const section = "accessibility-characteristic-groups";
	const paths = await loadStaticPaths(section);
	return paths;
}

const {id} = Astro.params;
---
<IdGroupMembers id={id} section="accessibility-characteristic-groups" subsection="accessibility-characteristics" title="Object Characteristic Group"></IdGroupMembers>

