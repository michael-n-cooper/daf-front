---
import MatrixItemDetails from '../../components/MatrixItemDetails.astro';
import { loadStaticPaths } from '../../components/load-static-paths';

export async function getStaticPaths() { 
	const section = "accessibility-characteristics";
	const paths = await loadStaticPaths(section);
	return paths;
}

const {id} = Astro.params;
---
<MatrixItemDetails id={id} section="accessibility-characteristics" type="Object Characteristic"></MatrixItemDetails>

